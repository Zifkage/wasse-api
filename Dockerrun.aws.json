{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "server",
      "image": "zifkage/multi-server",
      "hostname": "api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "links": ["mongo"]
    },
    {
      "name": "mongo",
      "image": "mongo:latest",
      "hostname": "mongo",
      "essential": false,
      "memory": 128
    }
  ]
}
